{
  "name": "anewstead-cra-mui",
  "version": "0.1.0",
  "description": "Create-React-App material-ui redux",
  "private": true,
  "author": "Andrew Newstead",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build;",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:html": "prettier --write $PWD/'/{src,public}/**/*.{htm,html,hbs}'",
    "lint:js": "eslint --fix $PWD/'/src/**/*.{js,jsx,ts,tsx,json}'; prettier --write $PWD/'/src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:style": "stylelint --fix $PWD/'/src/**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run lint:js; npm run lint:style; npm run lint:html"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "dompurify": "^2.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-react-parser": "^1.2.8",
    "import-sort-style-eslint": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "just-detect-adblock": "^1.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "react": "^17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.1",
    "react-scripts": "^4.0.3",
    "react-slick": "^0.28.1",
    "redux": "^4.1.1",
    "slick-carousel": "^1.8.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=12"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
