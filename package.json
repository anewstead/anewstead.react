{
  "name": "anewstead-cra-mui",
  "version": "2.0.0",
  "description": "Create-React-App material-ui redux",
  "private": true,
  "author": "Andrew Newstead",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install; git config core.ignorecase false;",
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host --open --port 3003",
    "dev-sb": "storybook dev --port 6006",
    "build": "tsc && vite build",
    "build-sb": "storybook build",
    "analyze": "tsc && vite build --mode development",
    "start": "vite preview --host --port 3003",
    "start-sb": "node -e \"console.log('http://localhost:6006')\"; http-server storybook-static --port 6006 -s",
    "start-sb-test": "WAIT_ON_TIMEOUT=9999 start-server-and-test start-sb 6006 test-sb",
    "e2e": "cd e2e && playwright test",
    "test": "run-s test-sb test-js cover",
    "test-js": "jest --coverage",
    "test-sb": "test-storybook --coverage",
    "test-ci": "CI=true jest --maxWorkers=2",
    "cover": "run-s cover-merge cover-report",
    "cover-merge": "istanbul-merge --out coverage/merged/coverage-merged.json ./coverage/storybook/coverage-storybook.json  ./coverage/jest/coverage-jest.json",
    "cover-report": "nyc report --reporter=text --reporter=json --temp-dir=./coverage/merged --report-dir=./coverage/report",
    "lint": "run-s lint-js lint-style pretty;",
    "lint-js": "eslint --fix $PWD/'/{src,e2e}/**/*.{js,jsx,ts,tsx,json}' --cache",
    "lint-style": "stylelint --fix $PWD/'/src/**/*.{css,scss,sass,less,js,jsx,ts,tsx}' --cache",
    "pretty": "prettier --write $PWD/'/{src,e2e}/**/*.{js,jsx,ts,tsx,json,html,htm,hbs,xml}' --loglevel warn --cache"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@mui/utils": "^5.11.13",
    "@reduxjs/toolkit": "^1.9.3",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.3.4",
    "dompurify": "^3.0.1",
    "html-react-parser": "^3.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
