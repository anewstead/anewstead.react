{
  "name": "anewstead-cra-mui",
  "version": "1.0.0",
  "description": "Create-React-App material-ui redux",
  "private": true,
  "author": "Andrew Newstead",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "e2e": "cd e2e && jest",
    "eject": "react-scripts eject",
    "analyze": "npm run build; source-map-explorer 'build/static/js/*.js'",
    "pretty": "prettier --write $PWD/'/{src,e2e}/**/*.{js,jsx,ts,tsx,json}' --loglevel warn --cache",
    "lint:js": "eslint --fix $PWD/'/{src,e2e}/**/*.{js,jsx,ts,tsx,json}' --cache",
    "lint:style": "stylelint --fix $PWD/'/src/**/*.{css,scss,js,jsx,ts,tsx}' --cache",
    "lint:html": "prettier --write $PWD/'{src,public}/**/*.{htm,html,hbs}' --no-error-on-unmatched-pattern",
    "lint": "npm run lint:js; npm run lint:style; npm run lint:html; npm run pretty;",
    "prepare": "husky install; git config core.ignorecase false;"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.7",
    "@mui/utils": "^5.10.6",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^0.27.2",
    "chrome-finder": "^1.0.7",
    "css-mediaquery": "^0.1.2",
    "deepmerge": "^4.2.2",
    "dompurify": "^2.4.0",
    "html-react-parser": "^3.0.4",
    "jest-localstorage-mock": "^2.4.22",
    "just-detect-adblock": "^1.1.0",
    "msw": "^0.47.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "tss-react": "^4.1.4",
    "web-vitals": "^3.0.2"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/dompurify": "^2.3.4",
    "@types/expect-puppeteer": "^5.0.1",
    "@types/jest": "^29.1.0",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.23",
    "@types/puppeteer": "^5.4.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-check-file": "^1.2.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "import-sort-style-eslint": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "import-sort-style-renke": "^6.0.0",
    "jest-puppeteer": "^6.1.1",
    "postcss": "^8.4.16",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-sort-json": "^0.0.3",
    "puppeteer-core": "^18.1.0",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    "last 2 versions",
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "renke",
      "parser": "typescript"
    }
  },
  "jest": {
    "resetMocks": false
  }
}
