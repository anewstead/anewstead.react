    "html-react-parser": "^5.1.9",
    "mapbox-gl": "^3.2.0",
    "markdown-to-jsx": "^7.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-lazily": "^0.9.2",
    "react-map-gl": "^7.1.7",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "usehooks-ts": "^3.0.2",
    "web-vitals": "^3.5.2"
