{
  "name": "anewstead-react",
  "version": "3.0.0",
  "description": "vite typescript material-ui redux",
  "private": true,
  "author": "Andrew Newstead",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install; git config core.ignorecase false;",
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host --open --port 3003",
    "dev-sb": "storybook dev --port 6006",
    "build": "tsc && vite build",
    "build-sb": "storybook build --quiet --loglevel warn",
    "analyze": "tsc && vite build --mode development",
    "start": "vite preview --host --port 3003",
    "start-sb": "http-server storybook-static --port 6006 -s; node -e \"console.log('http://localhost:6006')\";",
    "start-sb-test": "WAIT_ON_TIMEOUT=9999 start-server-and-test start-sb 6006 test-sb",
    "e2e": "playwright test",
    "test": "run-s test-sb test-js",
    "test-js": "rimraf ./coverage/jest-js; jest --coverage",
    "test-sb": "rimraf ./coverage/storybook; test-storybook --coverage",
    "test-ci": "CI=true jest --maxWorkers=2 --coverage=false",
    "cover": "run-s cover-js cover-sb",
    "cover-js": "nyc report --reporter=html --temp-dir=./coverage/jest-js --report-dir=./coverage/jest-js/report; node -e \"console.log('REPORT:','file://'+process.cwd()+'/coverage/jest-js/report/index.html')\";",
    "cover-sb": "nyc report --reporter=html --temp-dir=./coverage/storybook --report-dir=./coverage/storybook/report; node -e \"console.log('REPORT:','file://'+process.cwd()+'/coverage/storybook/report/index.html')\";",
    "lint": "run-s lint-js lint-style pretty;",
    "lint-js": "eslint --fix $PWD/'/{src,e2e}/**/*.{js,jsx,ts,tsx,json}' --cache",
    "lint-style": "stylelint --fix $PWD/'/src/**/*.{css,scss,sass,less,js,jsx,ts,tsx}' --cache",
    "pretty": "prettier --write $PWD/'/{src,e2e}/**/*.{js,jsx,ts,tsx,json,html,htm,hbs,xml}' --log-level warn --cache"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.8",
    "@mui/utils": "^5.14.8",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.5.0",
    "clsx": "^2.0.0",
    "dompurify": "^3.0.5",
    "html-react-parser": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
