    "lint:style": "stylelint --fix $PWD/'/src/**/*.{css,scss,sass,less,js,jsx,ts,tsx}' --cache",
    "lint": "npm run lint:js; npm run lint:style; npm run pretty;",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install; git config core.ignorecase false;",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
